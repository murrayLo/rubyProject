<!-- This is a partial for the enroll student and edit student forms -->

<%= notice %>

<%= form_for(@student) do |f| %>

    <p><label>First Name: </label><%= f.text_field :fname %></p>
    <p><label>Surname: </label><%= f.text_field :sname %></p>
    <p><label>Course Title: </label><%= f.text_field :course %></p>
    
    <%= f.submit %>
    
    <% end %>